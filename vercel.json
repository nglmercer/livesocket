{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/assets/(.*)",
        "dest": "/public/assets/$1"
      },
      {
        "src": "/components/(.*)",
        "dest": "/public/components/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ]
  }
  